  {
    "name": "Hextent",
    "version": "2.0",
    "description": "A wonderful new colour with every new tab. Make your day full of colour with Hextent",
    "manifest_version": 2,

    "chrome_url_overrides" : {
      "newtab": "index.html"
    },
    
    "browser_action": {
      "default_popup": "popup.html"
    },

    "icons": {
      "16": "Hextent.png",
      "48": "Hextent.png",
      "128": "Hextent.png"
    }
  }
